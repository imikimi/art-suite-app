params: "[pipelineName]"
action: ({args:[pipelineName]}) ->
  if pipelineName
    &ArtEry.pipelines[pipelineName].getInitializeParams()
  else
    &ArtStandardLib.Promise.deepAll each pipeline in &ArtEry.pipelines into out = {} when pipeline.getInitializeParams
      out[pipeline.pipelineName] = pipeline.getInitializeParams()
